<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="486.0" prefWidth="559.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TextField fx:id="messageTextField" layoutX="179.0" layoutY="302.0" onAction="#sendMessage" prefHeight="91.0" prefWidth="318.0" text="" />
      <Button fx:id="sendButton" layoutX="414.0" layoutY="407.0" mnemonicParsing="false" onAction="#sendMessage" text="Send" />
      <Button fx:id="attachButton" layoutX="414.0" layoutY="267.0" mnemonicParsing="false" text="Attach" />
      <ListView fx:id="messageList" layoutX="179.0" layoutY="67.0" prefHeight="200.0" prefWidth="318.0" />
      <Label fx:id="receiverName" layoutX="178.0" layoutY="40.0" prefHeight="26.0" prefWidth="318.0" text="" />
      <VBox layoutX="34.0" layoutY="40.0" prefHeight="352.0" prefWidth="143.0">
         <children>
            <TextField fx:id="searchUserTextfield" onAction="#sendUserName" prefHeight="32.0" prefWidth="143.0" />
            <ListView  fx:id="userOnlineList" prefHeight="335.0" prefWidth="143.0" onMousePressed="#selectReceiver" />
         </children>
      </VBox>
      
      
   </children>
</AnchorPane>
